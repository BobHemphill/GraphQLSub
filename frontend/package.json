{
  "name": "GraphqlSub",
  "version": "1.0.0",
  "description": "WS communication with GraphQL Subscriptions and React Relay",
  "author": "Robert J Hemphill <bob.hemphill@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "relay": "relay-compiler --src ./src --schema ./src/relay/schema.graphql",
    "relay-update": "./node_modules/.bin/get-graphql-schema http://localhost:3000/graphql > src/relay/schema.graphql & yarn relay",
    "lint": "eslint \"{src/**/*.js,}\" --quiet"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^14.3.1",
    "prettier": "^1.17.1",
    "relay-compiler": "^4.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "dependencies": {
    "apollo-link": "^1.2.11",
    "apollo-link-ws": "^1.0.17",
    "found": "^0.3.21",
    "found-relay": "0.4.0-alpha.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-relay": "^4.0.0",
    "subscriptions-transport-ws": "^0.9.16"
  }
}
